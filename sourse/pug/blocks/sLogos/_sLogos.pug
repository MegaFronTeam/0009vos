mixin sLogos(data)
	// start sLogos
	+b.SECTION.sLogos.section#sLogos&attributes(attributes)
		.container
			+b.section-title.text-center
				h2  Наши партнеры
				
			.row.gy-4
				- for (var y = 0; y < 3; y++)
					- for (var x = 1; x < 7; x++)
						+e.col.col-xl-2.col-lg-3.col-sm-4.col-6
							+pic(`logo-${x}.png`,'',0)
	// end sLogos